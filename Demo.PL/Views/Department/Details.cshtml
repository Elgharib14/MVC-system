@using Demo.DAL.Model;
@model Department
@{
    ViewData["Title"] = "Details";
}

<h1> Details</h1>


<form class="container" asp-action="Creat">
    <div class="form-group">
        <label class="col-form-label" asp-for="Code"></label>
        <input  asp-for="Code" class="form-control" readonly />
      
    </div>

    <div class="form-group">
        <label class="col-form-label" asp-for="Name"></label>
        <input asp-for="Name" class="form-control" readonly />
       
    </div>

    <div class="form-group">
        <label class="col-form-label" asp-for="DateTimeCreation"></label>
        <input asp-for="DateTimeCreation" class="form-control" readonly />
       
    </div>
    <br />

    <center>
        <a asp-controller="Department" asp-action="Index" class="btn btn-primary" >BackToHome</a>
        <a asp-controller="Department" asp-action="Edit" class="btn btn-success" asp-route-id="@Model.Id">Edit</a>
        <a asp-controller="Department" asp-action="Delete" class="btn btn-warning" asp-route-id="@Model.Id">Delet</a>
    </center>
</form>

